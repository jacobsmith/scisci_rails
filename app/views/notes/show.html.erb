<p id="notice"><%= notice %></p>

<p>
  <strong>Quote:</strong>
  <%= @note.quote %>
</p>

<p>
  <strong>Comments:</strong>
  <%= @note.comments %>
</p>

<p>
  <strong>Tags:</strong>
<% @note.tags.each do |tag| %>
  <%= link_to tag, project_tags_path(@note.source.project, tag) %>
  &nbsp
<% end %>
</p>

<%= link_to 'Edit', edit_note_path(@note) %> |
<%= link_to 'Back', source_notes_path(@note.source) %>
